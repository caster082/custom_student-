<odoo>
    <data>
        <record id="tree_bio_history" model="ir.ui.view">
            <field name="name">tree.bio.history</field>
            <field name="model">bio.history</field>
            <field name="arch" type="xml">
                <tree string="Bio History">
                    <field name="name"/>
                    <field name="position"/>
                    <field name="responsibilities"/>
                    <field name="from_date"/>
                    <field name="to_date"/>
                     <field name="description"/>
                </tree>

            </field>



        </record>

    </data>
</odoo>